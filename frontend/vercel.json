{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
       { "source": "/api/(.*)", "destination": "https://anu-chatbot.onrender.com/api/$1" },
       { "source": "/(.*)", "destination": "/index.html" }
    }
  ]
}
